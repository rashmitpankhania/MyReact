{
    "extends": "airbnb",
    "rules":{
        "no-console":"off",
        "no-param-reassign":["error", {"props": false}],
        "global-require":"off",
        "no-underscore-dangle": ["error", { "allow": ["_id", "_metadata"] }],
        "no-alert":"off",
        "import/extensions":"off",
        "import/no-extraneous-dependencies":"off",
        "react/prop-types":"off"
    },
    "env": {
        "browser": true
    }
}